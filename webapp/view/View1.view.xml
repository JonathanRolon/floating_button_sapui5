	<Shell id="shell">
		<App id="app">
			<pages>
				<Page id="page" title="{i18n>title}" showFooter="true" showSubHeader="false">
					<content/>
					<footer>
						<OverflowToolbar style="Clear" design="Transparent">
							<content>
								<ToolbarSpacer/>
