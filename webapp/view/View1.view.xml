<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" controllerName="ns.floating_button.controller.View1"
	displayBlock="true">
	<Shell id="shell">
		<App id="app">
			<pages>
				<Page id="page" title="{i18n>title}" showFooter="true"
					floatingFooter="true">
					<content/>
					<footer>
						<Toolbar style="Clear" design="Transparent">
							<core:HTML id="_miCoreHTML"/>
						</Toolbar>
					</footer>
				</Page>
			</pages>
		</App>
	</Shell>
</mvc:View>